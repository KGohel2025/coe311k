    "import math\n",
    "\n",
    "run = True; \n",
    "x = int(input(\"Enter a number: \"))\n",
    "\n",
    "den= 2\n",
    "div = math.trunc(x/den)\n",
    "ans = x - (den *div)\n",
    "answer = str(ans)\n",
    "x = div\n",
    "while x>1: \n",
    "    \n",
    "\n",
    "    den= 2\n",
    "\n",
    "    div = math.trunc(x/den)\n",
    "    ans = x - (den *div)\n",
    "    answer += str(ans)\n",
    "    x=div\n",
    "\n",
    "print (answer)"
